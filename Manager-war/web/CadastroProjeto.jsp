		<div id="content" class="content">
			<!-- begin page-header -->
			<h1 class="page-header">Projeto  <small>Cadastre seu novo projeto</small></h1>
			<!-- end page-header -->
			
			<!-- begin row -->
			<div class="row">
                            <!-- begin col-6 -->
			    <div class="col-md-6">
			        <!-- begin panel -->
                    <div class="panel panel-inverse" data-sortable-id="form-validation-1">
                        <div class="panel-heading">
                            <div class="panel-heading-btn">
                                <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                                <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
                                <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                                <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
                            </div>
                            <h4 class="panel-title">Projeto</h4>
                        </div>
                        <div class="panel-body panel-form">
                            <form class="form-horizontal form-bordered" data-parsley-validate="true" name="demo-form" action="FrontController" method="POST">
                                <div class="form-group">
                                        <label class="control-label col-md-3 col-sm-3" for="nome">Nome *:</label>
                                        <div class="col-md-7 col-sm-7">
                                                <input class="form-control" type="text" id="nome" name="nome" placeholder="Nome do Projeto" data-parsley-required="true" />
                                        </div>
                                        <label class="control-label col-md-3 col-sm-3" for="descricao">Descrição *:</label>
                                        <div class="col-md-7 col-sm-7">
                                            <textarea class="form-control" id="descricao" name="descricao" rows="3" placeholder="Descrição do Projeto" data-parsley-required="true" /></textarea>
                                        </div>
                                </div>
                                <div class="form-group">
                                        <label class="control-label col-md-3 col-sm-3" for="datainicio">Data início *:</label>
                                        <div class="col-md-7 col-sm-7">
                                            <input class="form-control" type="text" id="data1" name="datainicio"/>
                                        </div>
                                        <label class="control-label col-md-3 col-sm-3" for="datainicio">Data final *:</label>
                                        <div class="col-md-7 col-sm-7">
                                            <input class="form-control" type="text" id="data2" name="datafinal"/>
                                        </div>
                                </div>
                                
                                <input type="hidden" name="command" value="ProjetoCommand_criar"/>
                               
                                <div class="form-group">
                                    <label class="control-label col-md-3 col-sm-3">Equipe</label>
                                    <div class="col-md-7 col-sm-7">    
                                        <c:forEach var="c" items="${colaborador}">
                                            <input type="checkbox" name="colaborador" value="${c.getCdUsuario()}"/>${c.getNome()}<br>
                                        </c:forEach>               
                                    </div>
                               </div>
                                <div class="form-group">
                                        <label class="control-label col-md-3 col-sm-3"></label>
                                        <div class="col-md-7 col-sm-7">
                                                <button type="submit" class="btn btn-primary">Criar</button>
                                        </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <!-- end panel -->
                </div>
                <!-- end col-6 -->
                <!-- begin col-6 -->
            <div class="col-md-6">
            </div>
